{
    "workbench.startupEditor": "newUntitledFile",
    "sync.gist": "11d75dab7b3f568d8b43b48e66735254",
    "workbench.colorTheme": "Dracula",
    "workbench.iconTheme": "vscode-great-icons",
    "editor.multiCursorModifier": "ctrlCmd",
    "terminal.integrated.fontSize": 13,
    "blade.format.enable": true,
    "window.title": "${rootName} ðŸš€ ${activeEditorMedium}",
    "breadcrumbs.enabled": true,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "showMusicMetrics": false,
    "showGitMetrics": false,
    "showWeeklyRanking": false,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "terminal.integrated.rendererType": "dom",
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.minimap.renderCharacters": false,
    "editor.minimap.maxColumn": 200,
    "editor.minimap.showSlider": "always",
    "zenMode.hideLineNumbers": false,
    "zenMode.centerLayout": false,
    "gitlens.mode.active": "zen",
    "yaml.schemas": {
        "file:///home/aleksa/.vscode/extensions/me-dutour-mathieu.vscode-github-actions-2.2.7/schemas/workflow.schema.json": "/.github/workflows/*.yml",
        "file:///home/aleksa/.vscode/extensions/me-dutour-mathieu.vscode-github-actions-2.2.7/schemas/action.schema.json": "/action.yml"
    },
    "php.validate.executablePath": "/usr/bin/php",
    "extensions.ignoreRecommendations": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "files.associations": {
        "*.stub": "php"
    },

    // Plantuml server to generate UML diagrams on-the-fly.
    "plantuml.server": "http://www.plantuml.com/plantuml",
    "[javascript]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "workbench.editor.wrapTabs": true,
    "terminal.integrated.scrollback": 10000,
    "editor.tabSize": 2,
    "aws.profile": "profile:default",
    "cSpell.userWords": [
        "backoff",
        "mediaobject",
        "remock",
        "retryable",
        "spyupdate",
        "talpanetwork",
        "upsert"
    ],
    "cSpell.enableFiletypes": [
        "feature",
        "shellscript"
    ],
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "[python]": {
        "editor.tabSize": 4,
        "gitlens.codeLens.symbolScopes": [
            "!Module"
        ]
    },
    "workbench.sideBar.location": "right",
    "editor.fontFamily": "'Cascadia Code'",
    "editor.fontLigatures": true
}